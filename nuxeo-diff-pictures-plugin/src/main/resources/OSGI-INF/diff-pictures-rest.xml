<?xml version="1.0"?>
<component name="rg.nuxeo.diff.pictures.restAPI.contrib">

  <extension target="org.nuxeo.ecm.platform.ui.web.restAPI.service.PluggableRestletService"
    point="restlets">

    <documentation>
     GET /nuxeo/restAPI/diffPicturesResult/{leftDocId}/{rightDocId}
    </documentation>

    <restletPlugin
      name="diffPicturesResult"
      class="org.nuxeo.diff.pictures.DiffPicturesResultRestlet"
      enabled="true"
      useSeam="true">
      <urlPatterns>
        <urlPattern>/diffPicturesResult/{leftDocId}/{rightDocId}</urlPattern>
      </urlPatterns>
    </restletPlugin>
  </extension>

</component>
